<!DOCTYPE html>
<html lang="es">

<head>
    <%- include("./partials/head.ejs") %>
</head>

<body>
    <header>
        <%- include("./partials/header.ejs") %>
        <%- include("./partials/nav.ejs") %>
    </header>
    <main>
        <div class="blog content">
            <h2>All Tests</h2>
            <% if (blogs.length > 0) { %>
            <% blogs.forEach(blog => { %>

            <h3 class="title"><%= blog.title %></h3>
            <ul>
                <li><%= blog.snippet %></li>
                <li><%= blog.body %></li>
            </ul>

            <% }) %>
            <% } else { %>
            <p>There are no blogs to display...</p>
            <% } %>

        </div>

    </main>
    <footer>
        <%- include("./partials/footer.ejs") %>
    </footer>
</body>

</html>